<UserControl x:Class="ToscaAssistant.Pages.SubPages.Files"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Pages/Windows/Operations/createfile.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Create File" Source="/Pages/Windows/Operations/createfile.xaml"/>
                <mui:Link DisplayName="Open File" />
                <mui:Link DisplayName="Delete File" />
                <mui:Link DisplayName="Copy File" />
                <mui:Link DisplayName="File Existence" />
                <mui:Link DisplayName="File Compare" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
