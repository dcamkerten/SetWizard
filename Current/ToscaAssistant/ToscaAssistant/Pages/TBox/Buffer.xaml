<UserControl x:Class="ToscaAssistant.Pages.TBox.Buffer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Pages/TBox/functions/setBuffer.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Name to Buffer" />
                <mui:Link DisplayName="Delete Buffer" />
                <mui:Link DisplayName="Set Buffer" Source="/Pages/TBox/functions/setBuffer.xaml"/>
                <mui:Link DisplayName="Partial Buffer" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
