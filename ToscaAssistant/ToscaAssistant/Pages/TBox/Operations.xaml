<UserControl x:Class="ToscaAssistant.Pages.TBox.Operations"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Expression Evaluation" />
                <mui:Link DisplayName="Convert Decimal" />
                <mui:Link DisplayName="Wait" />
                <mui:Link DisplayName="Start Timer" />
                <mui:Link DisplayName="Stop Timer" />
                <mui:Link DisplayName="String Functions" />

            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
